{
  "name": "darylcarr.com",
  "version": "1.0.0",
  "description": "Personal site of Daryl Carr",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/scss:dist/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass-site": "sass --no-source-map src/scss/style.scss dist/css/style.css",
    "build:sass": "npm-run-all --parallel build:sass-site",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r",
    "start": "cross-env ELEVENTY_ENV=dev npm-run-all build:sass --parallel watch:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.8"
  }
}
